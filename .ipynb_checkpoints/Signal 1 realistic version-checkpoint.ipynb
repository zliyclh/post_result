    "def revision_calc(ticker,date,EPS_df,period):\n",
    "    '''\n",
    "    Calculate EAR from ticker and reference date\n",
    "    '''\n",
    "    if type(date)==pd._libs.tslib.NaTType:\n",
    "        return None\n",
    "    elif type(date)==pd._libs.tslib.Timestamp:\n",
    "        date=date.strftime(\"%d/%b/%Y\")\n",
    "    elif type(date)==str:\n",
    "        date=date\n",
    "    \n",
    "    if ticker in EPS_df.index:\n",
    "        eps_series=EPS_df.loc[ticker]\n",
    "        date_series=eps_series.index.tolist()\n",
    "        if date in date_series:\n",
    "            day0=date_series.index(date)\n",
    "            post_series=eps_series.iloc[day0:]\n",
    "            pre_series=eps_series.iloc[:day0]\n",
    "            try:\n",
    "                revision=post_series[period-1]/pre_series[-1]-1\n",
    "            except:\n",
    "                revision=None\n",
    "            return revision\n",
    "        else:\n",
    "            return None\n",
    "    else:\n",
    "        return None"
   ]
